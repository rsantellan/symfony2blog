    <head>
        <meta charset="UTF-8" />
    <title>{% trans %}admin_page_title{% endtrans %}</title>
    <link rel="icon" type="image/x-icon" href="{{ asset('bundles/maithcommonadmin/images/favicon.ico') }}" />

    <script type="text/javascript" src="{{ asset('bundles/maithcommonadmin/js/jquery-1.10.1.min.js') }}"></script>    
    <script type="text/javascript" src="{{ asset('bundles/maithcommonadmin/js/jquery-migrate-1.2.1.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/maithcommonadmin/js/jquery.infieldlabel.min.js') }}"></script>    
    
    
	{% block stylesheets %}{% endblock %}
	{% block javascripts %}{% endblock %}
    
    </head>
<body class="hubspot">  

    <div class="page">

            {% if is_granted("IS_AUTHENTICATED_REMEMBERED") %}
                {{ 'layout.logged_in_as'|trans({'%username%': app.user.username}, 'FOSUserBundle') }} |
                <a href="{{ path('fos_user_security_logout') }}">
                    {{ 'layout.logout'|trans({}, 'FOSUserBundle') }}
                </a>
            {% else %}
                <a href="{{ path('fos_user_security_login') }}">{{ 'layout.login'|trans({}, 'FOSUserBundle') }}</a>
            {% endif %}
        {% for type, messages in app.session.flashbag.all() %}
            {% for message in messages %}
                <div class="flash-{{ type }}">
                    {{ message }}
                </div>
            {% endfor %}
        {% endfor %}

        {% block fos_user_content %}
        {% endblock fos_user_content %}
    
    </div>
      <footer>
        <div>
            <span class="customer-app-info">
                
                

                    <!-- PRODUCTION -->
            </span>


        </div>
    </footer>
    </body>
</html>
