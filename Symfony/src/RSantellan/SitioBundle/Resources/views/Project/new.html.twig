{% extends 'RSantellanSitioBundle:Admin:layout.html.twig' %}

{% block body -%}
    <h1>Project creation</h1>

    <form action="{{ path('rsproject_create') }}" method="post" {{ form_enctype(form) }}>
        {{ form_row(form.name)}}
        
        {{ form_row(form.cliente)}}
        
        {{ form_row(form.tipo_de_trabajo)}}
        
        {{ form_row(form.description)}}
        
        {{ form_row(form.category)}}

        {#{ form_widget(form) }#}
        
        {{ form_errors(form) }}
        {{ form_rest(form) }}


        <p>
            <button type="submit">Create</button>
        </p>
    </form>

        <ul class="record_actions">
    <li>
        <a href="{{ path('rsproject') }}">
            Back to the list
        </a>
    </li>
</ul>
{% endblock %}
