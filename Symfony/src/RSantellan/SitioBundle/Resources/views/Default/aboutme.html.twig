	<!-- START PAGE WRAPPER -->
	<div class="container main-wrapper">
		<div id="main-content" class="sixteen columns">
			<div class="row remove-bottom clearfix">
                <img alt="" src="{{ 'web/images/100_3863.JPG' | mImage(283, 262, 'rce', true) }}" class="border alignright" title="Align Right" width="200" />
                {% trans %}rsantellan_site_about_me_first_paragraph{% endtrans %}
			<!--
            <div class="alignleft caption">
                <img alt="" src="{{ 'web/images/100_3750.JPG' | mImage(283, 262, 'rce', true) }}" class="border" title="Align Left" width="150" />
                {% trans %}rsantellan_site_about_me_second_image_caption{% endtrans %}
			</div>
            -->    
            {% trans %}rsantellan_site_about_me_second_paragraph{% endtrans %}
			</div>
		</div><!-- main-content -->
		
		
	</div><!-- .container -->

		
	<!-- END BLOG WRAPPER -->
{% endblock %}    