{% extends 'RSantellanSitioBundle::layout.html.twig' %}
{% block body %}
<!-- START SEPARATOR  -->
	<div id="separator">
		<div class="btop-1px"></div>
		<div class="container">
			<!-- start separator -->
			<div class="sixteen columns">
				<h4 class="page-title">{% trans %}rsantellan_site_about_me_title{% endtrans %}</h4>
			</div><!-- sixteen columns -->
		</div><!-- .container -->
		<div class="bbottom-1px"></div>
	</div><!-- #separator -->
