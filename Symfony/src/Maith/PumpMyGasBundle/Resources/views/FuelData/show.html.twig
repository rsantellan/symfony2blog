{% extends '::base.html.twig' %}

{% block body -%}
    <h1>FuelData</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Fueldate</th>
                <td>{{ entity.fueldate|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Price</th>
                <td>{{ entity.price }}</td>
            </tr>
            <tr>
                <th>Fuelquantity</th>
                <td>{{ entity.fuelquantity }}</td>
            </tr>
            <tr>
                <th>Kilometers</th>
                <td>{{ entity.kilometers }}</td>
            </tr>
            <tr>
                <th>Kilometerperliter</th>
                <td>{{ entity.kilometerperliter }}</td>
            </tr>
            <tr>
                <th>Priceperliter</th>
                <td>{{ entity.priceperliter }}</td>
            </tr>
            <tr>
                <th>Notes</th>
                <td>{{ entity.notes }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('admin_pump_fueldata') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('admin_pump_fueldata_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
